{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off",
        "html-lang-valid": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/aria": [
      "default",
      {
        "aria-command-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "overflow-anchor",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color",
          "scroll-behavior"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "meta-charset-utf-8": "off"
  }
}